<div class="row m-0 p-0">
<div class="col-2 bg-dark m-0 p-0">
</div>
<div class="col-7">
<h1>Log in</h1>

<div>
  <div>
    <%= form_for(:session, url: login_path) do |f| %>

      <%= f.text_field :name %>

      <%= f.submit "Log in" %>
      <%= link_to 'Sign up', new_user_path %>
    <% end %>

  </div>
</div>
</div>
</div>
